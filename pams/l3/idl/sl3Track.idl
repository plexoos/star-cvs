/*   sl3Track
 * Table containing tracks found by l3 tracking     
*/
   struct sl3Track{
<<<<<<< sl3Track.idl
      float    id      ;       // id
      float    xReference ;    // x reference for conformal mapping
      float    yReference ;    // y reference for conformal mapping
      float    s11Xy  ;       // Fit parameters
      float    s12Xy  ;       // conformal line in xy plane
      float    s22Xy  ;
      float    g1Xy   ;
      float    g2Xy   ;
      float    s11Sz  ;      // Fit parameters in sz plane
      float    s12Sz  ;
      float    s22Sz  ;
      float    g1Sz   ;
      float    g2Sz   ;
      float    trackLength ;
   };
