/*   thm_evaluation
 * Control thm_evaluation
 */
struct thm_evaluation {
   long    rowSector ; /* Sector * 100 + row            */
   long    mcId      ; /* Id geant hit */
   long    recoId    ; /* Id reconstructed hit */
   float   r         ; /* r Mc hit */
   float   phi       ; /* phi reconstructed hit */
   float   z         ; /* z Mc hit */
   float   drphi     ; /* difference in phi  */
   float   dz        ; /* difference in z    */
};







