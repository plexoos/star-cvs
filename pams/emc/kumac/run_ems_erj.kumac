MACRO run_ems_erj [ems]='/dui/data/emc' _
		  [jet]='/dui/data/emc'  

MESSAGE '>>>>>>>>>>>>>>>> Calling ems_erj <<<<<<<<<<<<<<<<'
ami/call ems_erj       [ems]/ems_control _
                       [jet]/ems_emecor _
		       [jet]/ems_jetpar _
                       [jet]/ems_jetlist
