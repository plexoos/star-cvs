MACRO run_jet_erj	[par]='/dui/constants/emc/params' _
                        [jet]='/dui/data/emc/tracks'

MESSAGE '>>>>>>>>>>>>>>>> Calling emc_erj <<<<<<<<<<<<<<<<'
ami/module/call emc_erj _       
		       [par]/ems_control _
                       [jet]/emc_energy _
		       [par]/emc_jetpar _
                       [jet]/ems_jetlist
