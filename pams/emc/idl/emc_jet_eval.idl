/*****************************************************************************
*   Table: emc_jet_eval
*        description: contains the parton, jet(event genarator loverl) and
*                     jet(after geant) informations 
*        OGAWA, Akio
*/
   struct emc_jet_eval {
       long      njet1;     /* # of jet found at event generation level */
       long      njet2;     /* # of jet found at event full simulated */
       long      flag1[4];  /* flag */
       long      np1[4];    /* # of track in jet found at event generation level */
       float     et1[4];    /* Et of jet found at event generation level */
       float     eta1[4];   /* Eta of jet found at event generation level */  
       float     phi1[4];   /* Phi of jet found at event generation level */   
       long      flag2[4];  /* flag */
       long      np2[4];    /* # of cell in jet found after Geant */
       float     et2[4];    /* Et of jet found after Geant */
       float     eta2[4];   /* Eta of jet found after Geant */
       float     phi2[4];   /* Phi of jet found after Geant */
       float     det[4];    /* Et difference */
       float     deta[4];   /* Eta difference */
       float     dphi[4];   /* Phi difference */
   };
