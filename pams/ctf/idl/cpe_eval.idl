struct cpe_eval {  
       long      id;         /* key */
       long      id_globtrk; /* Pointer to global track */
       long      ieta;       /* eta index */
       long      iphi;       /* phi index */
       long      mhits;      /* # mc hits in slat */
       long      mtrk     ;  /* Monte Carlo track that fired slat */
       long      nrow;       /* # rows in tracks */
       long      mother ;    // Mother particle hits slat
       long      nslat;      /* # slats associated with tracks */
       long      q;          /* Track charge */
       long      pid;        /* Slat pid */
       long      mvtx ;      /* Monte Carlo vertex */
       long      tof_id;     /* Link to tof_pid */
       float     beta;       /* Measured beta */
       float     drphi;      /* Distance impact slat in rphi */
       float     dz;         /* Distance impact slat in z */
       float     dz_lit;     /* Measured distance impact to tube */
       float     eta;        /* Pseudorapidity */
       float     msleng;     /* mc trajectory length */
       float     meta;       /* Mc eta */
       float     mtof;       /* Mc tof */
       float     mptot ;     /* Contributors momentum at vertex */
       float     mphi;       /* Dominant contributor phi angle */
       float     mxhit;      /* Mc x hit */
       float     myhit;      /* Mc y hit */
       float     mzhit;      /* Mc z hit */
       float     nmip;       /* # mip */
       float     p;          /* Reconstructed track momentum */
       float     phi;        /* azimuthal angle */
       float     rimp;       /* Track impact parameter */
       float     sleng;      /* Track length */
       float     tdedx;      /* dedx from tpc */
       float     tof_ns[3];  /* Tof # sigmas for pi,K,p */
       float     tof_pr[3];  /* Tof # sigmas for pi,K,p */
       float     tpc_ns[3];  /* Tof # sigmas for pi,K,p */
       float     tpc_pr[3];  /* Tof # sigmas for pi,K,p */
       float     traw;       /* Uncorrected time */
       float     tcor;       /* Corrected time */
       float     x_dis;      /* Extrapolated distance */
       float     x_extra;    /* Extrapolated x */
       float     y_extra;    /* Extrapolated y */
       float     z_extra;    /* Extrapolated y */
       float     zimp;       /* Z distance track extrapolation and ip */
   } ;
