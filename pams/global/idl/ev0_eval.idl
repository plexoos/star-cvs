/*   ev0_eval.h
*   Table: ev0_eval
*/
   struct ev0_eval {
	long id; /* Primary key (same as in dst_v0_vertex) */
	long itrue; /* Evaluation flag for vertex */
	long id_parent; /* id (geant) of parent track (g2t_track.id)*/
	long id_pos; /* id (geant) of pos track (g2t_track.id) */
	long id_neg; /* id (geant) of neg track (g2t_track.id) */
	long vid; /* vertex id (geant) (g2t_vertex.id) */
	long pid_true; /* true pid (geant) of parent track */
	long pid_pos_true; /* true pid (geant) of pos track */
	long pid_neg_true; /* true pid (geant) of neg track */
	float x_true; 
	float y_true; /* Real decay vertex */
	float z_true; 
	float pos_px_true;
	float pos_py_true; /* Real parent momentum */
	float pos_pz_true;
	float neg_px_true;
	float neg_py_true; /* Real parent momentum */
	float neg_pz_true;
	float rapidity_true; /* Real rapidity of parent */
   } ;
