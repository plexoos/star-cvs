/*   svm_tpc_eval.idl
*   Table: svm_eval_tpc
*/
    struct svm_eval_tpc { 
       long      id;         /* tpc track id, = tpeval.rtrk */
       long      pid;        /* Geant pid # from MC: mkine */
       long      q;          /* charge from tptrack */
       long      status;     /* values 1-7, tracking & matching status */
       float     e;          /* total energy using MC mass and tptrack */
       float     et;         /* transverse energy using MC mass and tpc */
       float     eta;        /* pseudorapidity from values in tptrack */
       float     pt;         /* transverse momentum from tptrack */
       float     ptot;       /* total momentum from tptrack */
       float     y;          /* rapidity using MC mass and tptrack */
   } ;

