/*   svm_eval_svt.idl
*   Table: svm_eval_svt
*/
   struct svm_eval_svt {
       long      id;         /* svt track id = svt_ctrack.id */
       long      pid;        /* Geant pid # from Monte Carlo: mkine */
       long      q;          /* charge from svt_track */
       long      status;     /* values 1-7, tracking & matching status */
       float     e;          /* total energy from MC mass and svt_track */
       float     et;         /* transverse energy from MC mass and svt */
       float     eta;        /* pseudorapidity from svt_track values */
       float     pt;         /* transverse momentum from svt_track */
       float     ptot;       /* Total momentum from svt_track */
       float     y;          /* rapidity from values in svt_track */
   } ;
