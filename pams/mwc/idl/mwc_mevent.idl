 *   Table: mwc_mevent
 *         description: Event table for MWC simulator
 */

   struct mwc_mevent {         
       long       cal_hits;   /* Number of calculted total hits from wire */
       long       no_hits;    /* number of hits on MWC */
       long       no_wires;   /* Number of wires hit in an event */
       long       no_tracks;  /* Number of tracks hitting MWC */
   };

