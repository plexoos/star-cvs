/*   mwc_sector.idl
*                                           
*        Description: MWC Monte Carlo sector information
*/

   struct mwc_sector { 
       long       index;      /* Index for Sectors */
       long       nhit;       /* # charged particles going through sect. */
       long       sector;     /* sector index */
       long       tot_hit;    /*  total num hits on sector */
       long       iphi;       /* phi index for sector location */
       long       ieta;       /* eta index for sector location */
       float      de;         /* Deposited energy */       
   };
