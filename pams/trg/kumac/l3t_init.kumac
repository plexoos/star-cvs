macro l3_init
macro/global/import *


cd [params]/l3

newtable  l3t_para l3t_tpc_para 1 
rowcount l3t_para 1
*
putvalue 'l3t_para[0].infoLevel'       0
putvalue 'l3t_para[0].FirstSector'     1
putvalue 'l3t_para[0].LastSector'     24  
putvalue 'l3t_para[0].InnerMostRow'    1
putvalue 'l3t_para[0].OuterMostRow'   45
putvalue 'l3t_para[0].NPrimaryLoops'   1
putvalue 'l3t_para[0].NSecondaryLoops' 1
putvalue 'l3t_para[0].ErrorScaleSz'    1.
putvalue 'l3t_para[0].ErrorScaleXy'    1.
putvalue 'l3t_para[0].Phimin'          0.
putvalue 'l3t_para[0].Phimax'        360.
putvalue 'l3t_para[0].Etamin'        0.0
putvalue 'l3t_para[0].Etamax'        2.2
putvalue 'l3t_para[0].PhiSlices'      10
putvalue 'l3t_para[0].EtaSlices'      40
putvalue 'l3t_para[0].BField'          0.5
putvalue 'l3t_para[0].SFitSz'          1
putvalue 'l3t_para[0].SPhiClosed'      0
putvalue 'l3t_para[0].SDPhiLimit'      0.08 
putvalue 'l3t_para[0].SDEtaLimit'      0.08
putvalue 'l3t_para[0].SChi2Cut'      100.0
