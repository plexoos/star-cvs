putvalue 'l3t_para[0].SMinimumHitsPerTrack'      5
putvalue 'l3t_para[0].SMaxSearchPadrowsTrack'    3
putvalue 'l3t_para[0].SMaxSearchPadrowsSegment'  2 
putvalue 'l3t_para[0].MergePrimaries'            1
putvalue 'l3t_para[0].NumberOfPsiSlices'        40
putvalue 'l3t_para[0].NumberOfTanLSlices'       40
putvalue 'l3t_para[0].MinSlicePsi'              0.0
putvalue 'l3t_para[0].MaxSlicePsi'             360.
putvalue 'l3t_para[0].MinSliceTanL'            -2.2
putvalue 'l3t_para[0].MaxSliceTanL'             2.2
putvalue 'l3t_para[0].SDPsiMaxMerge'            0.01
putvalue 'l3t_para[0].SDTanlMaxMerge'           0.02
*
*
*   Set phimin and max per sector
*
putvalue 'l3t_para[0].sectorPhiMin'      45.  15.   0. 315. 285. 255._
                                        225. 195. 165. 135. 105.  75._
                                        105. 135. 165. 195. 225. 255._
                                        285. 315.   0.  15.  45.  75.

putvalue 'l3t_para[0].sectorPhiMax'      75.  45.  30. 345. 315. 285._
                                        245. 225. 195. 165. 135. 105._
                                        135. 165. 195. 225. 255. 285_
                                        315. 345.  30.  45.  75. 105.

putvalue 'l3t_para[0].sectorPhiShift'     0.   0.  15.   0.   0.   0._
                                          0.   0.   0.   0.   0.   0._
                                          0.   0.   0.   0.   0.   0._
                                          0.   0.  15.   0.   0.   0.


putvalue 'l3t_para[0].sectorEtaMin'       0.   0.   0.   0.   0.   0._
                                          0.   0.   0.   0.   0.   0._
                                         -2.  -2.  -2.  -2.  -2.  -2._
