#!/bin/bash

#
# The signal handler kills the child processes
#

if [ $# -eq 0 ]; then
    echo "Usage:   launch_reanalyze <port> <run>";
    exit
fi

# get main function and shift args as appropriate...

run=$2
port=$1

echo "Main program:  port=$port run=$run"


launch JevpServerMain -port $port &

launch BasePlotBuilder -port $port -file $run &

launch HltBuilder -port $port -file $run &

