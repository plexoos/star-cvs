#!/usr/local/bin/perl
#use Env;
#use File::Basename;
print "St_dst_Maker::cons\n";
Import qw ( env INCLUDE LIB BIN OBJ );
my $FilePath = FilePath();
my $DirPath  = DirPath();
print "Execute ", $0;
print " in DirPath=",$DirPath," FilePath=",$FilePath,"\n"; 
my $tag =  $FilePath . "*.cxx";  
print "Tag:", $tag, "\n"; 
my @files = glob $tag; 
print "files = ",@files,"\n";
@_[0] = $env;
@_[1] = 'St_dst_Maker.so';
my $i=1;
foreach $file (@files) {
  print "File:",$file,"\n"; $i++; 
  @_[$i] = basename($file); 
} 
#Install $env $LIB, 'St_dst_Maker.so'; 
my $m = 0;
for ($m =0; $m<=$i;$m++) { print $m," ",@_[$m],"\n";}
while ($m =shift) { print $m," ",@_[$m],"\n";}
%cons::LinkedModule;
#&cons::Program;
&cons::Program;
#Install $env, 'St_dst_Maker.so';
