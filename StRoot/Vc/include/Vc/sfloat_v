#ifndef VC_SFLOAT_V
#define VC_SFLOAT_V

#include "vector.h"
#include "internal/namespace.h"

namespace Vc
{
#if VC_IMPL_SSE
  typedef VECTOR_NAMESPACE::Vector<VECTOR_NAMESPACE::float8> sfloat_v;
#else
  typedef VECTOR_NAMESPACE::Vector<float> sfloat_v;
#endif
  typedef sfloat_v::Mask sfloat_m;
} // namespace Vc

#undef VECTOR_NAMESPACE

#endif // VC_SFLOAT_V
