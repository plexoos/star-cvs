
include ../rtsmakefile.def

CCFLAGS += -g 
#-DOBSOLETE
INCLUDES += -I../EVP_READER
LDLIBS = ../EVP_READER/libevp.2.0.a

%.o: %.cxx
	$(COMPILE.cc) $(OUTPUT_OPTION) $<

EVENT_TRACKER_OBJS = 	FtfFinder.o \
			l3CoordinateTransformer.o \
			FtfSl3.o \
			FtfDedx.o \
			FtfPara.o \
			FtfTrack.o \
			FtfBaseTrack.o \
			l3Swap.o \
			FtfBaseHit.o \
			FtfHit.o \
			FtfUtilities.o \
			gl3Event.o \
			gl3EMC.o \
			gl3Track.o \
			l3TrgReader.o \
			l3TrgReaderV20.o \
			l3TrgReaderV21.o \
			l3TrgReaderV12.o \
			gl3LMVertexFinder.o \
			gl3Hit.o \
			gl3Histo.o \
			l3BankUtils.o \
			l3EmcCalibration.o \
			gl3HistoManager.o \
			eventTrackerLib.o


all: eventTracker libeventTracker.a

libeventTracker.a: $(EVENT_TRACKER_OBJS)
	ar -cr libeventTracker.a $? 

install:
	$(INSTALL) eventTracker  ${BINDIR}
	$(INSTALL) libeventTracker.a ${LIBDIR}

eventTracker: libeventTracker.a

depend:
	$(MAKEDEPEND)

clean:
	rm -f core* *.o *.a *~ displayMap eventTracker

tar:
	rm -rf TRACKER
	mkdir TRACKER
	rm -f TRACKER/*
	cp *.cxx TRACKER
	cp *.h TRACKER
	cp *.hh TRACKER
	cp Makefile.tar TRACKER/Makefile
	tar -cvf evp_tracker.tar TRACKER
	$(INSTALL) evp_tracker.tar /afs/rhic/star/doc/www/daq/EVP_READER/


