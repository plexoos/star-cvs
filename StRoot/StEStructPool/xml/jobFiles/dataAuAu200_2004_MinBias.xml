<?xml version='1.0' encoding='UTF-8' ?>

<!-- This file contains information over-riding dataDefault for
     year 2004 minbias data analysis. -->
<jobDescription>
    <standardElements>
        <jobControl>
            <jobName widget='entry' id='jobName'>AuAu200_2004_MinBias</jobName>
            <outputDir widget='entry' id='outputDir'>$env(MYDATA)/Data/auau200/2004/MinBias</outputDir>
            <jobPurpose widget='text'>Test run over year 4, 200GeV AuAu minimum bias data.
            </jobPurpose>
        </jobControl>

        <starSubmit simulateSubmission='true'>
            <input URL="catalog:star.bnl.gov?production=P05ia,trgsetupname=ProductionMinBias,magscale=FullField,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename!~zerobias,storage!=HPSS" singleCopy="true" preferStorage="local" nFiles="all" />
            <input URL="catalog:star.bnl.gov?production=P05ia,trgsetupname=ProductionMinBias,magscale=ReversedFullField,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename!~zerobias,storage!=HPSS" singleCopy="true" preferStorage="local" nFiles="all" />
        </starSubmit>

        <eventCuts>
            <triggerTag Comment = 'Trigger tag for 2004 minbias AuAu data at 200GeV'>AuAu200GeVMinBias2004</triggerTag>
            <eventCut Comment = 'cut on position of primary vertex'>
                <cutName>primaryVertexZ</cutName>
                <Value widget = 'entry'>-30</Value>
                <Value widget = 'entry'>30</Value>
            </eventCut>
        </eventCuts>

        <doEStructMacro>
            <centralityInEtaOne id='centralityInEtaOne' widget='entry'>true</centralityInEtaOne>
            <centralities id='centralities' widget='entry'>1 18 39 73 123 191 280 392 532 707 815 1200</centralities>
            <ptCentralities id='ptCentralities' widget='entry'>1 18 39 73 123 191 280 392 532 707 815 1200</ptCentralities>
        </doEStructMacro>
    </standardElements>
</jobDescription>
