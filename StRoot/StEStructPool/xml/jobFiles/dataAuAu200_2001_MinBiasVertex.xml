<?xml version='1.0' encoding='UTF-8' ?>

<!-- This file contains information over-riding dataDefault for
     year 2001 minbias data analysis. -->
<jobDescription>
    <standardElements>
        <jobControl>
            <jobName widget='entry' id='jobName'>AuAu200_2001_MinBiasVertex</jobName>
            <outputDir widget='entry' id='outputDir'>$env(MYDATA)/standardElements/auau200/2001/MinBiasVertex</outputDir>
            <jobPurpose widget='text'>Test run over year 2, 200GeV AuAu minimum bias data.
                                      This data set was triggered using the MinBias Vertex trigger.
            </jobPurpose>
        </jobControl>

        <starSubmit simulateSubmission='true'>
            <input URL="catalog:star.bnl.gov?production=P02ge,collision=AuAu200,trgsetupname=MinBiasVertex,filetype=daq_reco_MuDst,sanity=1,storage!=HPSS,magscale=FullField" singleCopy="true" preferStorage="local" nFiles="all" />
            <input URL="catalog:star.bnl.gov?production=P02ge,collision=AuAu200,trgsetupname=MinBiasVertex,filetype=daq_reco_MuDst,sanity=1,storage!=HPSS,magscale=ReversedFullField" singleCopy="true" preferStorage="local" nFiles="all" />
        </starSubmit>

        <eventCuts>
            <triggerTag Comment = 'Trigger tag for 2001 minbias AuAu data at 200GeV'>AuAu200GeVMinBias2001</triggerTag>
            <eventCut Comment = 'cut on position of primary vertex'>
                <cutName>primaryVertexZ</cutName>
                <Value widget = 'entry'>-15</Value>
                <Value widget = 'entry'>15</Value>
            </eventCut>
        </eventCuts>

        <doEStructMacro>
            <centralities id='centralities' widget='entry'>7 18 38 71 121 192 289 415 575 770 887 1200</centralities>
            <ptCentralities id='ptCentralities'>7 18 38 71 121 192 289 415 575 770 887 1200</ptCentralities>
        </doEStructMacro>
    </standardElements>
</jobDescription>
