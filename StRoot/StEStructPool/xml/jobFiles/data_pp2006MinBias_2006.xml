<?xml version='1.0' encoding='UTF-8' ?>

<!-- This file contains information over-riding dataDefault for
     year 2006 200 GeV minbias pp data analysis. -->
<jobDescription>
    <standardElements>
        <jobControl>
            <jobName widget='entry' id='jobName'>pp2006MinBias_2006</jobName>
            <outputDir widget='entry' id='outputDir'>$env(MYDATA)/Data/pp200/2006MinBias_2006</outputDir>
            <jobPurpose widget='text'>Analyse 200GeV PP minimum bias 2006 data.
            </jobPurpose>
        </jobControl>

        <starSubmit simulateSubmission='true'>
            <input URL="catalog:star.bnl.gov?production=P06ie,trgsetupname=pp2006MinBias,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename!~zerobias,storage!=HPSS" singleCopy="true" preferStorage="local" nFiles="all" />
        </starSubmit>

        <eventCuts>
            <triggerTag Comment = 'Trigger tag for 2006 pp2006MinBias data at 200GeV'>pp2006MinBias2006</triggerTag>
            <eventCut Comment = 'cut on position of primary vertex'>
                <cutName>primaryVertexZ</cutName>
                <Value widget = 'entry'>-30</Value>
                <Value widget = 'entry'>30</Value>
            </eventCut>
        </eventCuts>

        <doEStructMacro>
            <centralities id='centralities' widget='entry'>1 4 7 10 13 25</centralities>
            <ptCentralities id='ptCentralities' typedef='const  double ptMultCut[] = ' typedef2='int mbNPtBins = '>1 4 7 10 13 25</ptCentralities>
        </doEStructMacro>
    </standardElements>
</jobDescription>
