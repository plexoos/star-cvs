<?xml version='1.0' encoding='UTF-8' ?>

<!-- This file contains information over-riding dataDefault for
     year 2004 minbias data analysis. -->
<jobDescription>
    <standardElements>
        <jobControl>
            <jobName widget='entry' id='jobName'>AuAu200_2004_ProductionHigh</jobName>
            <outputDir widget='entry' id='outputDir'>$env(MYDATA)/Data/auau200/2004/ProductionHigh</outputDir>
            <jobPurpose widget='text'>Test run over year 4, 200GeV AuAu production High.
            </jobPurpose>
        </jobControl>

        <starSubmit simulateSubmission='true'>
            <input URL="catalog:star.bnl.gov?production=P05ib||P05ic,trgsetupname=productionHigh,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename~physics,runnumber>5042040,storage!=HPSS" singleCopy="true" preferStorage="local" nFiles="all" />
        </starSubmit>

        <eventCuts>
            <triggerTag Comment = 'Trigger tag for 2004 productionHigh AuAu data at 200GeV'>AuAu200GeVProductionHigh2004</triggerTag>
            <eventCut Comment = 'cut on position of primary vertex'>
                <cutName>primaryVertexZ</cutName>
                <Value widget = 'entry'>-30</Value>
                <Value widget = 'entry'>30</Value>
            </eventCut>
        </eventCuts>

        <doEStructMacro>
            <centralities id='centralities' widget='entry'>1 18 39 73 123 191 280 392 532 707 815 1200</centralities>
            <ptCentralities id='ptCentralities' typedef='const  double ptMultCut[] = ' typedef2='int mbNPtBins = '>1 18 39 73 123 191 280 392 532 707 815 1200</ptCentralities>
        </doEStructMacro>
    </standardElements>
</jobDescription>
