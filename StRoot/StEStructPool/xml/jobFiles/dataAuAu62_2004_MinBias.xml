<?xml version='1.0' encoding='UTF-8' ?>

<!-- This file contains information over-riding dataDefault for
     year 2001 minbias data analysis. -->
<jobDescription>
    <standardElements>
        <jobControl>
            <jobName widget='entry' id='jobName'>AuAu62_2004_MinBias</jobName>
            <outputDir widget='entry' id='outputDir'>$env(MYDATA)/Data/auau62/2004/MinBias</outputDir>
            <jobPurpose widget='text'>Test run over year 4, 62GeV AuAu minimum bias data.
            </jobPurpose>
        </jobControl>

        <starSubmit simulateSubmission='true'>
            <input URL="catalog:star.bnl.gov?production=P04id,trgsetupname=production62GeV,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename!~zerobias,storage!=HPSS" singleCopy="true" preferStorage="local" nFiles="all" />
            <input URL="catalog:star.bnl.gov?production=P04ie,trgsetupname=production62GeV,filetype=daq_reco_MuDst,tpc=1,sanity=1,filename!~zerobias,storage!=HPSS" singleCopy="true" preferStorage="local" nFiles="all" />
        </starSubmit>

        <eventCuts>
            <triggerTag Comment = 'Trigger tag for 2004 minbias AuAu data at 62GeV'>AuAu62GeVMinBias2004</triggerTag>
            <eventCut Comment = 'cut on position of primary vertex'>
                <cutName>primaryVertexZ</cutName>
                <Value widget = 'entry'>-30</Value>
                <Value widget = 'entry'>30</Value>
            </eventCut>
        </eventCuts>

        <doEStructMacro>
            <centralities id='centralities' widget='entry'>0 9 23 45 79 127 193 279 392 541 635 825</centralities>
            <ptCentralities id='ptCentralities' widget='entry'>0 9 23 45 79 127 193 279 392 541 635 825</ptCentralities>
        </doEStructMacro>
    </standardElements>
</jobDescription>
