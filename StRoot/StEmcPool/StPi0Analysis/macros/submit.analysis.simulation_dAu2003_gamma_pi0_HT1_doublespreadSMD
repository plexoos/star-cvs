#!/bin/bash

export ANALYSIS_NAME='Simulated dAu2003 HT1 photon analysis with simulated pi0 weights, 2x of SMD calibration spread'
export ANALYSIS_TAG='simulation_dAu2003'
export ANALYSIS_SUBTAG=$ANALYSIS_TAG'_gamma_pi0_HT1_doublespreadSMD'
export ANALYSIS_DATA=$DATA_PATH'/simu_dAu2003_gamma_HT1_doublespreadSMD/*.root'
export ANALYSIS_CONFIG='weight_1gamma_pi0 useHT1 dAu2003 simulation nocentral gamma'
export ANALYSIS_WEIGHT='weights_dAu2003_'$WEIGHT_ITERATION'.root'
export ANALYSIS_OUTDIR=$DATAOUT_PATH'/simu_dAu2003_gamma_pi0_HT1_doublespreadSMD'
export ANALYSIS_FINALDIR=$DATAFINAL_PATH'/'
