<?xml version="1.0" encoding="utf-8" ?>
<job simulateSubmission="false" maxFilesPerProcess="&mymaxfiles;" minFilesPerProcess="&myminfiles;" filesPerHour="&myfilesperhour;" name="&mytag;">
    <command>
	starver &mystarver;
	cp -fr &mypwdanalysis;/StPi0Analysis/macros/&myrun; $SCRATCH/
	$SCRATCH/&myrun; \
'&mypwd;' \
'&mypwdanalysis;' \
'&mydebug;' \
'&myscript;' \
'&myscriptfinal;' \
'&mytag;' \
'&myconfig;' \
$SCRATCH \
'&mypathout;' \
'&mypathfinal;' \
$FILELIST \
$REQUESTID \
$JOBID \
$SUMS_nProcesses \
'&mydocons;' \
'&mydomake;' \
.$STAR_HOST_SYS \
'&myfinal;' \
'&myweightfile;' \
'&mysleep;'
    </command>
    <stdout discard="&mydiscardstdout;" URL="file:&mypathout;/&mystdout;"/>
    <input URL="&myquery;"/>
</job>
