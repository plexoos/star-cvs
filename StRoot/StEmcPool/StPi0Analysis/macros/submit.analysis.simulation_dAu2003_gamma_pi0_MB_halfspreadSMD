#!/bin/bash

export ANALYSIS_NAME='Simulated dAu2003 MB photon analysis with simulated pi0 weights, 1/2 of SMD calibration spread'
export ANALYSIS_TAG='simulation_dAu2003'
export ANALYSIS_SUBTAG=$ANALYSIS_TAG'_gamma_pi0_MB_halfspreadSMD'
export ANALYSIS_DATA=$DATA_PATH'/simu_dAu2003_gamma_MB_halfspreadSMD/*.root'
export ANALYSIS_CONFIG='weight_1gamma_pi0 useMB dAu2003 simulation nocentral gamma'
export ANALYSIS_WEIGHT='weights_dAu2003_'$WEIGHT_ITERATION'.root'
export ANALYSIS_OUTDIR=$DATAOUT_PATH'/simu_dAu2003_gamma_pi0_MB_halfspreadSMD'
export ANALYSIS_FINALDIR=$DATAFINAL_PATH'/'
