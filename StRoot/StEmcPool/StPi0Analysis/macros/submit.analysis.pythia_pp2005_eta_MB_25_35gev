#!/bin/bash

set_option 'files_per_job_analysis=1 files_per_hour_analysis=1.2 discard_stdout'

export ANALYSIS_NAME='Pythia pp2005 MB eta analysis, 25_35gev dataset'
export ANALYSIS_TAG='pythia_pp2005'
export ANALYSIS_SUBTAG=$ANALYSIS_TAG'_eta_MB_25_35gev'
export ANALYSIS_DATA=$DATA_PATH'/pythia_pp2005_MB_25_35gev/*.root'
export ANALYSIS_CONFIG='weight_pythia_25_35gev useMB pp2005 simulation pythia nocentral eta_sim'
export ANALYSIS_WEIGHT='weights_pp2005_'$WEIGHT_ITERATION'.root'
export ANALYSIS_OUTDIR=$DATAOUT_PATH'/pythia_pp2005_eta_MB_25_35gev'
export ANALYSIS_FINALDIR=$DATAFINAL_PATH'/'
