#!/bin/bash

set_option 'files_per_job_analysis=1 discard_stdout'

export ANALYSIS_NAME='Pythia pp2005 MB pi0 analysis, 2_3gev dataset'
export ANALYSIS_TAG='pythia_pp2005'
export ANALYSIS_SUBTAG=$ANALYSIS_TAG'_pi0_MB_02_03gev'
export ANALYSIS_DATA=$DATA_PATH'/pythia_pp2005_MB_02_03gev/*.root'
export ANALYSIS_CONFIG='weight_pythia_02_03gev useMB pp2005 simulation pythia nocentral'
export ANALYSIS_WEIGHT='weights_pp2005_'$WEIGHT_ITERATION'.root'
export ANALYSIS_OUTDIR=$DATAOUT_PATH'/pythia_pp2005_pi0_MB_02_03gev'
export ANALYSIS_FINALDIR=$DATAFINAL_PATH'/'
