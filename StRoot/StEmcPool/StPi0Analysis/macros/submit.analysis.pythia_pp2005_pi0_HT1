#!/bin/bash

set_option 'files_per_hour_analysis=100 files_per_job_analysis=100000 discard_stdout'

export ANALYSIS_NAME='Pythia pp2005 HT1 pi0 analysis, all pT datasets combined'
export ANALYSIS_TAG='pythia_pp2005'
export ANALYSIS_SUBTAG=$ANALYSIS_TAG'_pi0_HT1'
export ANALYSIS_DATA=\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_00_02gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_02_03gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_03_04gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_04_05gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_05_07gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_07_09gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_09_11gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_11_15gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_15_25gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_25_35gev/*.root '\
$DATAOUT_PATH'/pythia_pp2005_pi0_HT1_35_infgev/*.root '
export ANALYSIS_CONFIG='noweight useHT1 pp2005 simulation pythia nocentral processor_compare_ignore_weight'
export ANALYSIS_WEIGHT='weights_pp2005_'$WEIGHT_ITERATION'.root'
export ANALYSIS_OUTDIR=$DATAOUT_PATH'/pythia_pp2005_pi0_HT1'
export ANALYSIS_FINALDIR=$DATAFINAL_PATH'/'
