#!/bin/bash

set_option 'files_per_job_data=20 files_per_hour_data=3 eventroot xrootd=no discard_stdout'

export DATA_NAME='PYTHIA simulation using 2005 geometry, 2_3gev dataset rcf1231, HighTower-2 timestamps'
export DATA_TAG='pp2005Pythia'
export DATA_SUBTAG=$DATA_TAG'_HT2_02_03gev'
export DATA_QUERY='catalog:'$CATALOG'?runnumber=2000001231,production=P05ih,'$MYQUERY_general_sim
export DATA_CONFIG='config_pythia_pp2005_HT2.C'
export DATA_DIR=$DATA_PATH'/pythia_pp2005_HT2_02_03gev'
