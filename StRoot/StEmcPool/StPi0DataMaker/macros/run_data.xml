<?xml version="1.0" encoding="utf-8" ?>
<job simulateSubmission="false" fileListSyntax="&myfilelistsyntax;" maxFilesPerProcess="&mymaxfiles;" minFilesPerProcess="&myminfiles;" filesPerHour="&myfilesperhour;">
    <command>
starver &mystarver;
cp -fr &mypwdanalysis;/StPi0DataMaker/macros/&myrun; $SCRATCH/
$SCRATCH/&myrun; \
'&mypwd;' \
'&mypwdanalysis;' \
'&mydebug;' \
'&myscript;' \
'&mytag;' \
'&myconfig;' \
$SCRATCH \
$FILELIST \
$JOBID \
'&mydocons;' \
'&myfilelistsyntax;' \
'&mypathout;' \
'&myjprof;' \
'&mynevents;'
    </command>
    <stderr discard="&mydiscardstderr;" URL="file:&mypathout;/&mytag;_$JOBID.err"/>
    <stdout discard="&mydiscardstdout;" URL="file:&mypathout;/&mytag;_$JOBID.out"/>
    <input URL="&myquery;" nFiles="all"/>
</job>
