#!/bin/bash

set_option 'files_per_job_data=5 files_per_hour_data=0.5 eventroot xrootd=no discard_stdout'

export DATA_NAME='PYTHIA simulation using 2005 geometry, above_35gev dataset rcf1230, HighTower-1 timestamps'
export DATA_TAG='pp2005Pythia'
export DATA_SUBTAG=$DATA_TAG'_HT1_35_infgev'
export DATA_QUERY='catalog:'$CATALOG'?runnumber=2000001230,production=P05ih,'$MYQUERY_general_sim
export DATA_CONFIG='config_pythia_pp2005_HT1.C'
export DATA_DIR=$DATA_PATH'/pythia_pp2005_HT1_35_infgev'
