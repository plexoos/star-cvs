#!/bin/bash

set_option 'files_per_job_data=15 files_per_hour_data=0.5 eventroot xrootd=no discard_stdout'

export DATA_NAME='PYTHIA simulation using 2005 geometry, 25_35gev dataset rcf1229, dAu2003 MinBias timestamps'
export DATA_TAG='dAu2003FromPP2005Pythia'
export DATA_SUBTAG=$DATA_TAG'_MB_25_35gev'
export DATA_QUERY='catalog:'$CATALOG'?runnumber=2000001229,production=P05ih,'$MYQUERY_general_sim
export DATA_CONFIG='config_pythia_dAu2003_MB.C'
export DATA_DIR=$DATA_PATH'/pythia_dAu2003_MB_25_35gev'
