#!/bin/bash

export DATA_NAME='Pure MC pi0 simulation using 2003 geometry and BFC chain and MinBias timestamps, 1/2 of SMD calibration spread'
export DATA_TAG='dAu2003Simulation'
export DATA_SUBTAG=$DATA_TAG'_pi0_MB_halfspreadSMD'
export DATA_QUERY="file:$DATA_PATH/starsim_pi0_dAu2003/*.event.root"
export DATA_CONFIG='config_simulation_dAu2003_MB_halfspreadSMD.C'
export DATA_DIR=$DATA_PATH'/simu_dAu2003_pi0_MB_halfspreadSMD'
