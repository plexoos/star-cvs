#!/bin/bash

set_option 'files_per_job_data=20 files_per_hour_data=2.2 eventroot xrootd=no discard_stdout'

export DATA_NAME='PYTHIA simulation using 2005 geometry, 4_5gev dataset rcf1233, dAu2003 HighTower-1 timestamps'
export DATA_TAG='dAu2003FromPP2005Pythia'
export DATA_SUBTAG=$DATA_TAG'_HT1_04_05gev'
export DATA_QUERY='catalog:'$CATALOG'?runnumber=2000001233,production=P05ih,'$MYQUERY_general_sim
export DATA_CONFIG='config_pythia_dAu2003_HT1.C'
export DATA_DIR=$DATA_PATH'/pythia_dAu2003_HT1_04_05gev'
