macro g2t _
prefix=/star/mds/data/SD98/auau200/hijing135/default/b0_3/year1a/decay_only/Gstardata/set023/ _
first=1 _
no_files=10
 
list = $shell('chdir '//[prefix]//'; ls  *.fz');
*message [list]
nn = $words([list],' ')
i1 = [first]
i2 = [first]+[no_files]-1
if [i2]>[nn] then
  i2 = [nn]
endif
do i=[i1],[i2]
  file = $word([list],[i],1)
  message [file]
enddo
return