* 9/1/98 K. Turner (for 98g)
* Run BFC in following manner:
* - read in g2t file
* - run tpc fast sim 
* - run tpc tracking
* - write xdf file containing full directories

 exec $STAR/kumacs/chain/bfc _
   TOP=$STAR _
   tpc_sector_first=1 _
   tpc_sector_last=24 _
   gstar_settings=' field_only' _
   domain=' geometry tpc ' _
   chain='  g2tin tpg tfs tpt tte_e tid evout' _
   skip=0 _
   first=1 _
   no_events=1 _
   input_data_prefix=$AFS_RHIC/star/data/samples/ _
   input_data_file=auau_central_hijing.xdf _
   output_file=bfc_tpc_fast_sim_out _
   log =bfc_tpc_fast_sim.log
