/*    fpsMap.idl
*
*  Table: fpsMap
*
*       description: //  FPS QT map
*/ 

struct fpsMap { 
  unsigned short slatid;       /* 0-251: slat Id = (quad-1)*nLayer*maxSlat + (layer-1)*maxSlat + (slat-1) */
  short QTaddr;       /* 0-7 : QT Address, -1 for none existing slat */
  short QTch;         /* 0-31 : QT channel, -1 for none existing slat */
};  
