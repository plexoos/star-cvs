/* tofModuleConfig.idl
 *
 * Tables: tofModuleConfig
 *
 * description: // structure configuration for the Modules of MRPC-TOF
 */

struct tofModuleConfig {
  short iTray; /* number Id of the tray */
  short iModule; /* number Id(local in the tray) of the module */
  short iStatModule; /* status of this module */
  octet iStatCells; /* status of 6 cells in bitwise: (hightest)6-->1(lowest) */
  long iChannel[6]; /* daq channel Id of cell 1-6 (0-5) */
};
