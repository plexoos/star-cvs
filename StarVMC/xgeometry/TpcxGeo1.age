      ATTRIBUTE TSAW seen=1 colo=kYellow
      SHAPE pcon phi1=-15 dphi=30 nz=2 zi={204,
            TPCG_DzEnvelop-TPCG_MembTHK/2} rmn={48,48} rmx={208,
            208}
      dX2 = 1.470
      dX  = 0
      use TECW sec=1
      DO inOut = kInner, kOuter
      mycount = 0;
      use COOL inout=inOut
      use RIBS inout=inOut
      use HOLZ inout=InOut
      use TECW sec=(inOut+1)
      z2 = zWheel1
      z1 = z2 - TECW_Thick
