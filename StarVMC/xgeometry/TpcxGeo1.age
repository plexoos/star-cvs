      dz = (z1 - TPCG_MembTHK/2)/2
      x1 = -TECW_Height/2
      x2 =  TECW_Height/2
      y1 =  TECW_inwidth/2
      y2 =  TECW_ouwidth/2
      x0 =  (y1*x2-y2*x1)/(x2-x1)
      dYdX  =  (y2 - y1)/(x2 - x1)
      alpha = 180./Pi*ATAN(dYdX)
      r1 = y1/dYdX
      r2 = y2/dYdX
