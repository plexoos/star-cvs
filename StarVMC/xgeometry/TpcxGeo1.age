            zi={zzzSA(1),zzzSA(2)} rmn={rmnSA(inOut),rmnSA(inOut)},
            rmx={rmxSA(inOut),rmxSA(inOut)}
      ATTRIBUTE TSAS seen=0
      CREATE TALS
