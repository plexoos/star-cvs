      CREATE TSGT
      POSITION TSGT z=tsgt_zoffset 
      dy =(r2 - r1)/2
      y  =(r2 + r1)/2
      dx = TECW_widthRib/2
      dz =(TECW_Thick - TPCG_PadPlaneThickness - TECW_ThickAl)/2
      z  = z2 - dz
      dxW = TPCG_WireMountWidth/cos15/2
      dzW = TPCG_WireMountHeight/2
      xW = 0
      DO iRib = 0, 1
      iShape = kPara;
      myPar = {dx, dy, dz, -(1-2*iRib)*alpha}
      CREATE TRIB
      x  = ((y1 + y2)/2 - dx)*(1-2*iRib)
      POSITION TRIB x=y y=-x z=z ort=YX-Z 
      xW = ((y1 + y2)/2 -  dxW)*(1-2*iRib)
      myPar = {dxW, dy, dzW, -(1-2*iRib)*alpha, 0, 0}
      CREATE TWIR
      zW = z1 - dzW/2
      POSITION TWIR x=y y=-xW z=zW ort=YX-Z 
      END DO
      DO iRib = 0, TECW_noribs-1
      r   = r2 - RIBS_x  (irib+1) * INCH
      y   =      RIBS_y  (irib+1) * INCH
      dr  =      RIBS_dx (irib+1) * INCH
      dy  =      RIBS_dy (irib+1) * INCH
      IF ( dy<1.e-7 ) THEN
      dx2 =  r      *dYdX - TECW_widthRib - 2*dxW; ! TECW_widthLip
      dx1 = (r - dr)*dYdX - TECW_widthRib - 2*dxW; ! TECW_widthLip
      xc = r - TECW_zStepRib/2
      iShape = kTrd1;
      myPar = {dx1, dx2, dz, dr/2}
      CREATE TRIB
      POSITION TRIB x=r-dr/2 y=0 z=z2-dz ort=YZX 
      ELSE
      iShape = kBbox;
      myPar = {dr/2, dy/2, dz}
      CREATE TRIB
      POSITION TRIB x=r y=y z=z2-dz 
      ENDIF
      END DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWAS
      BLOCK TWAS   TpcWheelInnerAssembly & TpcWheelOuterAssembly TWAS and TWA1
      MEDIUM TWAS_STANDARD deemax=0.248853 stmin=1.15055
      SHAPE pgon phi1=-15 dphi=30 npdiv=1 nz=2,
            zi={zzzWA(1),zzzWA(2)} rmn={rmnWA(inOut),rmnWA(inOut)},
            rmx={rmxWA(inOut),rmxWA(inOut)}
      ATTRIBUTE TWAS seen=0
      dx =   TPCG_widTube*cm/2
      dz =   depthIOF(inOut + 1)/2
      dy =   heightIOF(inOut + 1)/2
      y1 = - dy
      y2 =   dy
      x1 =  width1IOF(inOut + 1)/2 - dx
      x2 =  width2IOF(inOut + 1)/2 - dx
      x0 =(y1*x2 - y2*x1)/(y2 - y1)
      dYdX  =(x2 - x1)/(y2 - y1)
      beta = 180./Pi*ATAN(dYdX)
      IF ( (inOut==kOuter) ) THEN
      dy -= 0.7
      ENDIF
      y  = xHoles(0) - dy
      x =(x1 + x2)/2
      DO iCoo = 0, 1
      myPar = {dx, dy, dz,-(1-2*iCoo)*beta, 0, 0}
      iShape = kPara;
      CREATE TCOO
      POSITION TCOO x=y y=x*(2*iCoo-1) z=zWheel1+dz ort=YX-Z 
      END DO
      DO icoo = 0, HOLZ_numberOfRows - 1
      x = xHoles(iCoo);         !// cm
      dx = TPCG_heigTube*cm/2
      dz = TPCG_widTube*cm/2
      dy = COOL_tubeLength( icoo+1 )*INCH / 2
      IF ( inOut==kInner ) THEN
      dy2 = dy + 2*dYdX*dx
      dy1 = dy;
      ELSE
      dy2 = dy
      dy1 = dy - 2*dYdX*dx
      ENDIF
      iShape = kTrd1;
      myPar = {dy1, dy2, dz, dx}
      myPar(11) = x-dx-TFEE_AssemblyThickness/2+dRSectorShift
      myPar(13) = zWheel1+dz
      CREATE TCOO
      POSITION TCOO x=myPar(11) z=myPar(13) ort=YZX 
      DO jcoo = 0, HOLZ_numberPerRow(icoo+1) - 1
      y = HOLZ_yholes( 9*icoo + jcoo + 1 )*INCH
      CREATE FEEA
      POSITION FEEA x=x+TFEE_AssemblyThickness/2+dRSectorShift y=y  _
	z=zWheel1+2*dz+TFEE_RibDZ konly='MANY' 
      END DO
      END DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- FEEA
      BLOCK FEEA   TGeoVolumeAssembly(FEE)
      MEDIUM FEEA_STD deemax=0.248853 stmin=1.15055
      ATTRIBUTE FEEA seen=1 colo=kGreen
      SHAPE box dx=TFEE_ass(1) dy=TFEE_ass(2),
            dz=TFEE_ass(3)
      ATTRIBUTE FEEA seen=1 colo=42
      CREATE FEEP
      POSITION FEEP 
      CREATE FEER
      POSITION FEER x=-TFEE_RibDX-TFEE_CardDX 
      CREATE FEEI
      POSITION FEEI x=2*TFEE_CardDX z=0.5 
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- FEEP
      BLOCK FEEP   FEEplate
      ATTRIBUTE FEEP seen=1 colo=kRed
      MATERIAL ALUMINIUM
      MEDIUM FEEP_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE box dx=TFEE_PlateDX dy=TFEE_PlateDY,
            dz=TFEE_PlateDZ
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- FEER
      BLOCK FEER   FEERib
      ATTRIBUTE FEER seen=1 colo=1
      MATERIAL ALUMINIUM
      MEDIUM FEER_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE box dx=TFEE_RibDX dy=TFEE_RibDY dz=TFEE_RibDZ
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- FEEI
      BLOCK FEEI   FEEitself
      ATTRIBUTE FEEI seen=1 colo=kViolet
      MATERIAL G10
      MEDIUM FEEI_g10 deemax=0.20313 stmin=0.0333806
      SHAPE box dx=TFEE_CardDX dy=TFEE_CardDY,
            dz=TFEE_CardDZ
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TALS
      BLOCK TALS   TpcSectorAlSupport the Sector G10 alliminium support
      ATTRIBUTE TALS seen=1 colo=kYellow
      MATERIAL ALUMINIUM
      MEDIUM TALS_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE pgon phi1=-alpha dphi=2*alpha npdiv=1,
            nz=2 zi={z1+TPCG_PadPlaneThickness,z1+TPCG_PadPlaneThickness+TECW_ThickAl},
            rmn={r1,r1},
            rmx={r2,r2}
      myPar(1)= TECW_holeDx
      myPar(2)= TECW_holeDy
      myPar(3)= TECW_ThickAl/2
      myPar(13)= zWheel1 - TECW_Thick + TPCG_PadPlaneThickness + TECW_ThickAl/2
      DO iTALS = 0, HOLZ_numberOfRows - 1
      myPar(11) = xHoles(iTALS); !// cm
      DO jtals = 0, HOLZ_numberPerRow( itals+1 ) - 1
      myPar(12) = HOLZ_yholes( 9*itals+jtals+1) *INCH
      CREATE THOL
      POSITION THOL x=myPar(11) y=myPar(12) z=myPar(13) 
      END DO
      END DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TSGT
      BLOCK TSGT   TpcSectorG10 the Sector G10
      ATTRIBUTE TSGT seen=1 colo=kBlue
      MATERIAL G10
      MEDIUM TSGT_G10 deemax=0.20313 stmin=0.0333806
      SHAPE pgon phi1=-alpha dphi=2*alpha npdiv=1,
            nz=2 zi={z1,z1+TPCG_PadPlaneThickness} rmn={r1,
            r1} rmx={r2,r2}
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWBT
      BLOCK TWBT   //WheelBottom
      ATTRIBUTE TWBT seen=1 colo=kGreen
      MATERIAL ALUMINIUM
      MEDIUM TWBT_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE pcon phi1=-15 dphi=30 nz=4 zi={zWheel1,
            zWheel1+3.500*INCH,zWheel1+3.500*INCH,TPCG_LengthW/2},
            rmn={TPCG_WheelIR,TPCG_WheelIR,
            TPCG_WheelIR+3.6,TPCG_WheelIR+3.6} rmx={TPCG_WheelR0,
            TPCG_WheelR0,TPCG_WheelR0,TPCG_WheelR0}
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWRI
      BLOCK TWRI   WheelOuterRing
      MEDIUM TWRI_standard stmin=1.15055
      SHAPE pcon phi1=-15 dphi=30 nz=4 zi={zWheel1,
            zTOFCend,zTOFCend,TPCG_LengthW/2} rmn={TPCG_WheelR2*cos15,
            TPCG_WheelR2*cos15,TPCG_WheelR2*cos15,
            TPCG_WheelR2*cos15} rmx={TPCG_WheelOR1,
            TPCG_WheelOR1,TPCG_WheelOR,TPCG_WheelOR}
      MEDIUM TWRI_something deemax=0.248853 stmin=1.15055
      ATTRIBUTE TWRI seen=0 colo=kGreen
      CREATE TWRC
      POSITION TWRC 
      myCos = cos15; myRmin=TPCG_WheelR2; myRmax = TPCG_WheelOR1
      myG1 = myRmin*cos15
      nCount = 0
      Do WHILE ( myg2 < myRmin  )
      ncount += 1
      myg2 = myRmin
      IF ( (myg2>myRMax*myCos) ) THEN
      myG2 = myRMax*myCos
      ENDIF
      alpha = ACOS(myCos)/Pi*180
      CREATE TWRG
      POSITION TWRG 
      myCos = myG2/myRmin; myG1=myG2
      IF ( (nCount>10) ) THEN
      break
      ENDIF
      End DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWRG
      BLOCK TWRG   WheelOuterRing PGON part
      ATTRIBUTE TWRG seen=1 colo=kGreen
      MATERIAL ALUMINIUM
      MEDIUM TWRG_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE pgon phi1=-alpha dphi=alpha*2 npdiv=1,
            nz=2 zi={zWheel1,TPCG_LengthW/2} rmn={myG1,
            myG1} rmx={myG2,myG2}
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWRC
      BLOCK TWRC   WheelOuterRing PCON part
      ATTRIBUTE TWRC seen=1 colo=kGreen
      MATERIAL ALUMINIUM
      MEDIUM TWRC_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE pcon phi1=-15 dphi=30 nz=4 zi={zWheel1,
            zTOFCend,zTOFCend,TPCG_LengthW/2} rmn={TPCG_WheelR2/cos15,
            TPCG_WheelR2/cos15,TPCG_WheelR2/cos15,
            TPCG_WheelR2/cos15} rmx={TPCG_WheelOR1,
            TPCG_WheelOR1,TPCG_WheelOR,TPCG_WheelOR}
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWTR
      BLOCK TWTR   TpcWheelTopRib
      ATTRIBUTE TWTR seen=1 colo=kBlue
      MATERIAL ALUMINIUM
      MEDIUM TWTR_aluminium deemax=0.18296 stmin=0.0334422
      myG1=TPCG_WheelR2 - (TPCG_WheelTotalRibWidth - TPCG_WheelRibWidth)/2
      SHAPE pgon phi1=-15 dphi=30 npdiv=1 nz=2,
            zi={zWheel1,zWheel1+TPCG_WheelRibHeight} rmn={myG1,
            myG1} rmx={TPCG_WheelR2,TPCG_WheelR2}
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWMR
      BLOCK TWMR   TpcWheelMiddleRib
      ATTRIBUTE TWMR seen=1 colo=kYellow
      MATERIAL ALUMINIUM
      MEDIUM TWMR_aluminium deemax=0.18296 stmin=0.0334422
      myG1=TPCG_WheelR2 - (TPCG_WheelTotalRibWidth - TPCG_WheelRibWidth)/2
      SHAPE pgon phi1=-15 dphi=30 npdiv=1 nz=4,
            zi={zWheel1,zWheel1+TPCG_WheelRibHeight,zWheel1+TPCG_WheelRibHeight,
            TPCG_LengthW/2} rmn={TPCG_WheelR1-TPCG_WheelTotalRibWidth/2,
            TPCG_WheelR1-TPCG_WheelTotalRibWidth/2,
            TPCG_WheelR1-TPCG_WheelRibWidth/2,
            TPCG_WheelR1-TPCG_WheelRibWidth/2},
            rmx={TPCG_WheelR1+TPCG_WheelTotalRibWidth/2,
            TPCG_WheelR1+TPCG_WheelTotalRibWidth/2,
            TPCG_WheelR1+TPCG_WheelRibWidth/2,TPCG_WheelR1+TPCG_WheelRibWidth/2}
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TRDO
      BLOCK TRDO   TpcRDOAssembly
      ATTRIBUTE TRDO seen=0
      ATTRIBUTE TRDO seen=1 colo=kYellow
      MEDIUM TRDO_whatever deemax=0.248853 stmin=1.15055
      SHAPE pgon phi1=-15 dphi=30 npdiv=1 nz=2,
            zi={zzzRDO(1),zzzRDO(2)} rmn={rmnRDO(1),rmnRDO(2)},
            rmx={rmxRDO(1),rmxRDO(2)}
      iShape = kPara;
      myPar = {TPCG_RDOCoolingdY, TPCG_RDOCoolingdX, TPCG_RDOCoolingdZ, 15}
      x = TPCG_WheelR2 - TPCG_RDOCoolingdX
      y = x*tan15 - TPCG_RDOCoolingdY
      z = TPCG_zRDO - 2*dy
      CREATE TCOO
      POSITION TCOO x=x y=y z=z ort=YX-Z 
      iShape = kPara;
      myPar = {TPCG_RDOCoolingdY, TPCG_RDOCoolingdX, TPCG_RDOCoolingdZ, -15}
      x = TPCG_WheelR2 - TPCG_RDOCoolingdX
      y = x*tan15 - TPCG_RDOCoolingdY
      z = TPCG_zRDO - 2*dy
      CREATE TCOO
      POSITION TCOO x=x y=-y z=z ort=YX-Z 
      RCoolingTube = TPCG_WheelR2 - TPCG_heigTube - 2*TPCG_dxRDO
      DO iCoo = 0, 7
      dz = TPCG_heigTube*cm/2
      dy = TPCG_widTube*cm/2
      z  = RCoolingTube
      dx1 = (z-dz)*tan15 - 2*TPCG_RDOCoolingdY
      dx2 = (z+dz)*tan15 - 2*TPCG_RDOCoolingdY
      iShape = kTrd1;
      myPar = {dx1, dx2, dy, dz}
      myPar(11) = z; myPar(13) = TPCG_zRDO - 2*dy
      CREATE TCOO
      POSITION TCOO x=myPar(11) y=0 z=myPar(13) ort=YZX 
      IF ( (iCoo .ne. 0 .and. iCoo .ne. 6) ) THEN
      myPar(11) = z + dz + TPCG_dxRDO;
      myPar(13) =          TPCG_zRDO-TPCG_dzRDO
      CREATE TRDC
      POSITION TRDC x=myPar(11) y=0 z=myPar(13) 
      ENDIF
      RCoolingTube -= dRDOCooling(iCoo)
      END DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TBRW
      BLOCK TBRW   WheelRibBox
      ATTRIBUTE TBRW seen=1 colo=kViolet
      MATERIAL ALUMINIUM
      MEDIUM TBRW_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE box dx=TPCG_WheelBoxDy/2 dy=TPCG_WheelBoxDx/2,
            dz=TPCG_WheelTHK/2
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWRB
      BLOCK TWRB   TpcWheelRib
      ATTRIBUTE TWRB seen=1 colo=kYellow
      MATERIAL ALUMINIUM
      MEDIUM TWRB_aluminium deemax=0.18296 stmin=0.0334422
      SHAPE para dx=myDx dy=myDy dz=myDz alph=myAlph,
            thet=0 phi=0
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TCOO
      BLOCK TCOO   CoolingTube
      ATTRIBUTE TCOO seen=1 colo=kRed
      MATERIAL Water_Pipe
      MEDIUM TCOO_h2o_pipe deemax=0.184746 stmin=0.0348298
      IF ( iShape==kPara ) THEN
      SHAPE para dx=myPar(1) dy=myPar(2) dz=myPar(3),
            alph=myPar(4) thet=0 phi=0
      ENDIF
      IF ( iShape==kTrd1 ) THEN
      SHAPE trd1 dx1=myPar(1) dx2=myPar(2) dy=myPar(3),
            dz=myPar(4)
      ENDIF
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TCAB
      BLOCK TCAB   Cables
      ATTRIBUTE TCAB seen=1 colo=kGreen
      MATERIAL Cables
      MEDIUM TCAB_Cables deemax=0.172513 stmin=0.0388322
      SHAPE para dx=myPar(1) dy=myPar(2) dz=myPar(3),
            alph=myPar(4) thet=0 phi=0
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TRDC
      BLOCK TRDC   RDOCard
      ATTRIBUTE TRDC seen=1 colo=kBlue
      MATERIAL G10
      MEDIUM TRDC_g10 deemax=0.20313 stmin=0.0333806
      SHAPE box dx=TPCG_dxRDO dy=TPCG_dyRDO dz=TPCG_dzRDO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TRIB
      BLOCK TRIB   Tpc Ribs
      ATTRIBUTE TRib seen=1 colo=kRed
      MATERIAL ALUMINIUM
      MEDIUM TRIB_aluminium deemax=0.18296 stmin=0.0334422
      IF ( iShape==kPara ) THEN
      SHAPE para dx=myPar(1) dy=myPar(2) dz=myPar(3),
            alph=myPar(4) thet=0 phi=0
      ELSEIF ( iShape==kTrd1 ) THEN
      SHAPE trd1 dx1=myPar(1) dx2=myPar(2) dy=myPar(3),
            dz=myPar(4)
      ELSEIF ( iShape==kBbox ) THEN
      SHAPE box dx=myPar(1) dy=myPar(2) dz=myPar(3)
      ENDIF
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TWIR
      BLOCK TWIR   WireMount
      ATTRIBUTE TWIR seen=1 colo=kYellow
      MATERIAL G10
      MEDIUM TWIR_g10 deemax=0.20313 stmin=0.0333806
      SHAPE para dx=myPar(1) dy=myPar(2) dz=myPar(3),
            alph=myPar(4) thet=0 phi=0
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- THOL
      BLOCK THOL   the Sector holes for FEE
      ATTRIBUTE THOL seen=1 colo=kGreen
      MATERIAL Air
      MEDIUM THOL_Air deemax=0.248853 stmin=1.15055
      SHAPE box dx=myPar(1) dy=myPar(2) dz=myPar(3)
      CREATE FEES
      POSITION FEES 
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- FEES
      BLOCK FEES   TFEESocket
      MATERIAL G10
      MEDIUM FEES_g10 deemax=0.20313 stmin=0.0333806
      ATTRIBUTE FEES seen=1 colo=kBlue
      SHAPE box dx=6.2/20 dy=57./20 dz=5.8/20
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TPGV
      BLOCK TPGV   is the Gas Volume placed in TPC
      ATTRIBUTE TPGV seen=1 colo=kRed
      MATERIAL P10
      MEDIUM TPGV_p10 deemax=0.248246 stmin=1.60594
      SHAPE tube rmin=tpgvIR rmax=tpcg_SenGasOR,
            dz=tpgvLeng/2
      DO iSecAng = 15, 360-15, 30
      CREATE TPSS
      POSITION TPSS alphaz=iSecAng konly='MANY' 
      END DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TPSS
      BLOCK TPSS   is a division of gas volume corresponding to a supersectors
      ATTRIBUTE TPSS seen=1 colo=kBlue
      MEDIUM TPSS_whatever deemax=0.248246 stmin=1.60594
      SHAPE tubs phi1=-15 phi2=15
      DO kase = 1, 2
      DO i_sec = 1, 2
      use TPRS sec=i_sec
      use TECW sec=i_sec
      zWheel1  = TPCG_LengthW/2 - TPCG_WheelTHK
      zBeg     = TPCG_MembTHK/2
      zEnd     = TPCG_zGroundGrid + TPRS_dAnode*2
      zDed     = TPCG_zGatingGrid - TPCG_DeadZone
      zPmt     = TPCG_zGatingGrid
      dx       = tprs_width/2
      IF ( kase==1 ) THEN
      dz = (zDed - zBeg)/2;
      z  = (zDed + zBeg)/2 - tpgvz
      ENDIF
      IF ( kase==2 ) THEN
      dz = (zEnd - zPmt)/2;
      z  = (zEnd + zPmt)/2 - tpgvz
      ENDIF
      DO i_row = 1, NINT(tprs_nRow)
      IF ( (i_row==1) ) THEN
      dy=tprs_npads(i_row)*tprs_pitch/2
      x=tprs_Rpads(i_row)-tprs_width
      CREATE TPAD
      POSITION TPAD x=x z=z dz=dz dx=dx dy=dy 
      ENDIF
      dy=tprs_npads(i_row)*tprs_pitch/2
      x=tprs_Rpads(i_row)
      CREATE TPAD
      POSITION TPAD x=x z=z dz=dz dx=dx dy=dy 
      IF ( (i_row==NINT(tprs_nRow)) ) THEN
      x=tprs_Rpads(i_row)+tprs_width
      dy=tprs_npads(i_row)*tprs_pitch/2
      CREATE TPAD
      POSITION TPAD x=x z=z dz=dz dx=dx dy=dy 
      ENDIF
      END DO
      END DO
      END DO
      ENDBLOCK
      ! ---------------------------------------------------------------------------------- TPAD
      BLOCK TPAD   is a REAL padrow WITH dimensions defined at positioning time
      if TPRS_width<1.5 {
      ATTRIBUTE TPAD seen=1 colo=2 serial=1
      }
      if TPRS_width>1.5 {
      ATTRIBUTE TPAD seen=1 colo=2 serial=2
      }
      MATERIAL p10
      MATERIAL sensitive_gas isvol=1 stemax=2.5*tprs_width
      MEDIUM TPAD_p10 stemax=2.5*tprs_width deemax=0.00175367 stmin=0.0249056 isvol=1
      SHAPE box dx=0 dy=0 dz=0
      Call GStPar(ag_imed, 'CUTGAM', 1e-4)
      Call GStPar(ag_imed, 'CUTELE', 1e-4)
      Call GStPar(ag_imed, 'DCUTE', 1e-4)
      Call GStPar(ag_imed, 'DCUTM', 1e-4)
      Call GStPar(ag_imed, 'STRA', 1.)
      HITS TPAD z:0.0005:s y:0.0005: x:0.0005: cx:10: cy:10: cz:10: lptot:18:(-3,2),
            sleng:0.1:(0,800) tof:16:(0,1.0E-6) lgam:16:(-2,+2) step:11:(0,10) user:21:(-0.01,0.01)
      ENDBLOCK
      <W> '===>> 	[End AgML Module TpcxGeo1]	 <<==='; (A32,/,/);
      END! Module TpcxGeo1
