      STRUCTURE TOFF { BoxWidth,SlatLen,Slat01z,Slat02z,Slat03z,Slat04z,Slat05z,Slat06z,
            Slat07z,Slat08z,Slat09z,Slat10z,SlatThck,SlatWid,SlatAng,PmtLen,PmtMaxR,PmtMinR,
